<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="634.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front.api.holerite.web.controller.ListaOrgaosController">
   <children>
      <SplitPane layoutX="14.0" layoutY="14.0" prefHeight="362.0" prefWidth="506.0">
         <items>
            <TableView fx:id="tableViewListaOrgaos" onMouseClicked="#selectItemEmpresa" prefHeight="281.0" prefWidth="504.0">
              <columns>
                <TableColumn fx:id="colCnpj" prefWidth="162.0" text="CNPJ" />
                <TableColumn fx:id="colNomeEmpresa" prefWidth="342.0" text="Descrição" />
              </columns>
            </TableView>
         </items>
      </SplitPane>
      <Button fx:id="btnAlterarRegistro" layoutX="547.0" layoutY="30.0" mnemonicParsing="false" onAction="#alterDataEmpresa" text="Alterar">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnSair" layoutX="547.0" layoutY="77.0" mnemonicParsing="false" onAction="#actionExitView" prefHeight="31.0" prefWidth="65.0" text="Sair">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
